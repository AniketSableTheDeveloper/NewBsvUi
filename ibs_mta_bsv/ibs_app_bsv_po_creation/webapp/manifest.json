{
    "_version": "1.59.0",
    "sap.app": {
        "id": "com.ibs.bsv.ibsappbsvpocreation",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.14.3",
            "toolsId": "a5f10aac-9b79-4a2f-bc3c-e71f02fa8184"
        },
        "dataSources": {
            "mainService": {
                "uri": "/odata/v2/ideal-bsv-purchase-order-srv/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/mainService/metadata.xml",
                    "odataVersion": "2.0"
                }
            },
            "ZIDEAL_ODATA_SALESORDER_SRV": {
                "uri": "/sap/opu/odata/sap/ZIDEAL_ODATA_SALESORDER_SRV",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/ZIDEAL_ODATA_SALESORDER_SRV/metadata.xml",
                    "odataVersion": "2.0"
                }
            }
        },
        "crossNavigation": {
            "inbounds": {
                "procreation-display": {
                    "semanticObject": "procreation",
                    "action": "display",
                    "title": "{{flpTitle}}",
                    "icon":"sap-icon://request",
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    }
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "dependencies": {
            "minUI5Version": "1.127.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.suite.ui.generic.template": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.ui.table": {},
                "sap.ushell": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.ibs.bsv.ibsappbsvpocreation.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            },
            "ZIDEAL_ODATA_SALESORDER_SRV": {
                "dataSource": "ZIDEAL_ODATA_SALESORDER_SRV",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false,
                    "metadataUrlParams": {
                        "sap-value-list": "none"
                    }
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "com.ibs.bsv.ibsappbsvpocreation.view",
                "controlAggregation": "beginColumnPages",
                "controlId": "layout",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteMaster",
                    "pattern": "Master/:Stockist:",
                    "target": [
                        "TargetMaster"
                    ]
                },
                {
                    "name": "Detail",
                    "pattern": "Detail/{PURCHASE_REQUEST_NO}",
                    "target": [
                        "TargetMaster",
                        "TargetDetail"
                    ]
                },
                {
                    "name": "RequestCreation",
                    "pattern": "",
                    "target": [
                        "RequestCreationTarget"
                    ]
                },
                {
                    "name": "createdorders",
                    "pattern": "CreatedRequest",
                    "target": [
                        "createdordersTarget"
                    ]
                },
                {
                    "name": "Scheme",
                    "pattern": "Scheme",
                    "target": [
                        "Scheme"
                    ]
                }
                
            ],
            "targets": {
                "TargetMaster": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Master",
                    "viewName": "Master",
                     "controlAggregation": "beginColumnPages"
                },
                "TargetDetail": {
                    "viewType": "XML",
                    "transition": "slide",
                    "viewId": "Detail",
                    "viewName": "Detail",
                    "controlAggregation": "midColumnPages"
                },
                "RequestCreationTarget": {
                    "viewType": "XML",
                    "transition": "slide",
                    "viewId": "RequestCreation",
                    "viewName": "RequestCreation"
                },
                "createdordersTarget": {
                    "viewType": "XML",
                    "transition": "slide",
                    "viewId": "createdorders",
                    "viewName": "createdorders",
                    "controlAggregation": "beginColumnPages"
                },
                "Scheme": {
                    "viewType": "XML",
                    "transition": "slide",
                    "viewId": "Scheme",
                    "viewName": "Scheme",
                    "controlAggregation": "beginColumnPages"
                }
            }
        },
        "rootView": {
            "viewName": "com.ibs.bsv.ibsappbsvpocreation.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "ibs_bs_bsv"
    }
}