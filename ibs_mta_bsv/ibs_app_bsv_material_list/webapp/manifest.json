{
  "_version": "1.60.0",
  "sap.app": {
    "id": "com.ibs.bsv.ibsappbsvmateriallist",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.17.5",
      "toolsId": "c6c89145-61b3-4343-9efd-06fdfc6cb57e"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZIDEAL_ODATA_SALESORDER_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "ideal-bsv-purchase-order-srv": {
        "uri": "/odata/v4/ideal-bsv-purchase-order-srv/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/ideal-bsv-purchase-order-srv/metadata.xml",
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
          "materiallist-display": {
              "semanticObject": "materiallist",
              "action": "display",
              "icon":"sap-icon://customer",
              "title": "{{flpTitle}}",
              "signature": {
                  "parameters": {},
                  "additionalParameters": "allowed"
              }
          }
      }
  }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.120.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.ibs.bsv.ibsappbsvmateriallist.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      },
      "ideal-bsv-purchase-order-srv": {
        "dataSource": "ideal-bsv-purchase-order-srv",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "com.ibs.bsv.ibsappbsvmateriallist.view",
        "async": true,
        "viewPath": "com.ibs.bsv.ibsappbsvmateriallist.view"
      },
      "routes": [
        {
          "name": "RouteView1",
          "pattern": ":?query:",
          "target": [
            "TargetView1"
          ]
        }
      ],
      "targets": {
        "TargetView1": {
          "id": "View1",
          "name": "View1"
        }
      }
    },
    "rootView": {
      "viewName": "com.ibs.bsv.ibsappbsvmateriallist.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "ibs_bs_bsv"
  }
}
